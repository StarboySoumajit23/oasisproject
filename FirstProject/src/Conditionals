import java.util.Scanner;

public class conditionals {
    public static void main(String[] args) {

    Scanner sc=new Scanner(System.in);

   int i=sc.nextInt();
    //  int i=8;
    if(i%2==0)
    {
        System.out.println("even");
    }
    else
    {
        System.out.println("odd");
    }
    }



}